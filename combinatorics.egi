(define $stirling2
  (lambda [$n $k]
    (sum
      (map
        (lambda $r (/ (* (power -1 (- k r)) (power r n)) (* (fact r) (fact (- k r)))))
        (between 1 k)))))

(define $bell 
  (lambda $n
    (sum
      (map
        (lambda $k (stirling2 n k))
        (between 0 n)))))

(define $catalan
  (lambda $n
    (/ (comb (* 2 n) n) (+ n 1))))


(map bell (between 1 9))
(map catalan (between 1 10))
